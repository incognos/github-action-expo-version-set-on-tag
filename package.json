    "detect-indent": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^27.5.1",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
